<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <!--查询所有的省份servlet声明开始-->
    <servlet>
        <servlet-name>OneSrevlet</servlet-name>
        <servlet-class>com.yunbocheng.controller.QueryProvinceServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>OneSrevlet</servlet-name>
        <url-pattern>/one</url-pattern>
    </servlet-mapping>
    <!--查询所有的省份servlet声明结束-->

    <!--注册查询city的servlet开始！-->
    <servlet>
        <servlet-name>TwoSrevlet</servlet-name>
        <servlet-class>com.yunbocheng.controller.QueryCityServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TwoSrevlet</servlet-name>
        <url-pattern>/two</url-pattern>
    </servlet-mapping>
    <!--注册查询city的servlet结束！-->
</web-app>